{{! <main>
	<h1>Welcome to KingUni Wiki!</h1> }}
<section>
	<h2>Login</h2>
	<div class="spacer">
		<form action="/login" method="POST">
			<label>Username:</label>
			<input
				type="text"
				id="username"
				name="username"
				value="{{username}}"
			/>
			<label>Password:</label>
			<input
				type="password"
				id="password"
				name="password"
				value="{{password}}"
			/>
			<input type="submit" value="Login" />
		</form>
	</div>
</section>
{{! </main> }}