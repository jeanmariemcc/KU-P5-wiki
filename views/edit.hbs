{{! <main>
	<h1>Welcome to KingUni Wiki!</h1> }}
<section>
	<h2>Edit article
	</h2>
	<div class="spacer">
		<form action="/edit/{{id}}" method="POST">
			<label>Article title</label>
			<h3>{{title}}</h3>
			<label>Content</label>
			<textarea name="description">{{description}}</textarea>
			<input type="submit" value="Submit" />
		</form>
	</div>
</section>
{{! </main> }}